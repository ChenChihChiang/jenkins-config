      <command>ANDROID_HOME=/usr/local/Android-SDK
export ANDROID_HOME

rm -rf /var/jenkins/workspace/Update-Service/build/outputs/apk/*

./gradlew clean build

cd /var/jenkins/workspace/Update-Service/build/outputs/apk

mv UpdateService_debug* UpdateService_debug.apk
